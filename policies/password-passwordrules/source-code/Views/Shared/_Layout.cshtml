        #attributeList ul li input[type=checkbox]:after {
            content: '';
            position: absolute;
            display: block;
            z-index: 1;
            width: 24px;
            height: 24px;
            border: 1px solid #605e5c;
            border-radius: 2px
        }

        #attributeList ul li input[type=checkbox]:before {
