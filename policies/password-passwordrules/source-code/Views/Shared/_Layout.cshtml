    function setPwdAttrs(pwd) {
        pwd.setAttribute('passwordrules', '@ViewData["rules"]');
    }

    function setupPwdAttrs() {
        var pwdFields = document.querySelectorAll('input[type=password]');
        for (var i = 0; i < pwdFields.length; i++) {
            setPwdAttrs(pwdFields[i]);
        }
    }

